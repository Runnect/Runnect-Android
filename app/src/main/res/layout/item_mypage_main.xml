<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <View
        android:id="@+id/view_my_page_main_frame"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/iv_my_page_main_star"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:src="@drawable/all_star"
        app:layout_constraintBottom_toBottomOf="@id/view_my_page_main_frame"
        app:layout_constraintStart_toStartOf="@id/view_my_page_main_frame"
        app:layout_constraintTop_toTopOf="@id/view_my_page_main_frame" />

    <TextView
        android:id="@+id/toolbar_my_main_page"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:text="목표 보상"
        app:layout_constraintBottom_toBottomOf="@id/view_my_page_main_frame"
        app:layout_constraintStart_toEndOf="@id/iv_my_page_main_star"
        app:layout_constraintTop_toTopOf="@id/view_my_page_main_frame" />

    <ImageView
        android:id="@+id/iv_my_page_main_arrow"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="15dp"
        android:src="@drawable/all_front_arrow"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>