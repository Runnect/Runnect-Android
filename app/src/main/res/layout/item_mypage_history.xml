<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>
        <!--        <variable-->
        <!--            name=""-->
        <!--            type="" />-->
    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="15dp"
        android:layout_marginBottom="10dp"
        tools:context=".presentation.mypage.MyHistoryActivity">

        <View
            android:id="@+id/view_myPage_history_frame"
            android:layout_width="match_parent"
            android:layout_height="166dp"
            android:background="@drawable/radius_9_22dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/iv_history"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_marginStart="13.83dp"
            android:layout_marginTop="11.92dp"
            app:layout_constraintStart_toStartOf="@id/view_myPage_history_frame"
            app:layout_constraintTop_toTopOf="@id/view_myPage_history_frame"
            tools:src="@drawable/history_iv_radius_9_22dp" />

        <TextView
            android:id="@+id/tv_history_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="12.9dp"
            android:layout_marginTop="18.51dp"
            android:text="석촌호수 한 바퀴"
            android:textSize="15.67sp"
            app:layout_constraintStart_toEndOf="@id/iv_history"
            app:layout_constraintTop_toTopOf="@id/iv_history" />

        <TextView
            android:id="@+id/tv_history_place"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="서울시 강동구"
            android:textSize="11.06sp"
            app:layout_constraintStart_toStartOf="@id/tv_history_title"
            app:layout_constraintTop_toBottomOf="@id/tv_history_title" />


        <View
            android:id="@+id/divider_vertical_rightOfPlace"
            android:layout_width="0.92dp"
            android:layout_height="10.61dp"
            android:layout_marginStart="4.99dp"
            android:background="@color/G2"
            app:layout_constraintBottom_toBottomOf="@id/tv_history_place"
            app:layout_constraintStart_toEndOf="@id/tv_history_place"
            app:layout_constraintTop_toTopOf="@id/tv_history_place" />

        <TextView
            android:id="@+id/tv_history_date"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="4.99dp"
            android:text="2022.12.28"
            android:textSize="11.06sp"
            app:layout_constraintStart_toEndOf="@id/divider_vertical_rightOfPlace"
            app:layout_constraintTop_toTopOf="@id/tv_history_place" />

        <com.google.android.material.divider.MaterialDivider
            android:id="@+id/divider_history"
            android:layout_width="match_parent"
            android:layout_height="0.92dp"
            android:layout_marginHorizontal="10dp"
            android:layout_marginTop="7.38dp"
            app:layout_constraintEnd_toEndOf="@id/view_myPage_history_frame"
            app:layout_constraintStart_toStartOf="@id/view_myPage_history_frame"
            app:layout_constraintTop_toBottomOf="@id/iv_history" />

        <!--총 거리-->

        <View
            android:id="@+id/view_myPage_distance_frame"
            android:layout_width="112dp"
            android:layout_height="66dp"
            app:layout_constraintBottom_toBottomOf="@id/view_myPage_history_frame"
            app:layout_constraintEnd_toStartOf="@id/view_myPage_time_frame"
            app:layout_constraintStart_toStartOf="@id/view_myPage_history_frame"
            app:layout_constraintTop_toBottomOf="@id/divider_history" />

        <TextView
            android:id="@+id/tv_total_distance"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="12.39dp"
            android:text="총 거리"
            app:layout_constraintEnd_toEndOf="@id/view_myPage_distance_frame"
            app:layout_constraintStart_toStartOf="@id/view_myPage_distance_frame"
            app:layout_constraintTop_toTopOf="@id/view_myPage_distance_frame" />

        <TextView
            android:id="@+id/tv_distance_data"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="4.03dp"
            android:text="4.01km"
            app:layout_constraintEnd_toEndOf="@id/view_myPage_distance_frame"
            app:layout_constraintStart_toStartOf="@id/view_myPage_distance_frame"
            app:layout_constraintTop_toBottomOf="@id/tv_total_distance" />

        <View
            android:id="@+id/divider_vertical_rightOfDistance"
            android:layout_width="0.92dp"
            android:layout_height="37.08dp"
            android:background="@color/G4"
            app:layout_constraintBottom_toBottomOf="@id/view_myPage_time_frame"
            app:layout_constraintStart_toStartOf="@id/view_myPage_time_frame"
            app:layout_constraintTop_toTopOf="@id/view_myPage_time_frame" />


        <!--이동시간-->

        <View
            android:id="@+id/view_myPage_time_frame"
            android:layout_width="112dp"
            android:layout_height="66dp"
            app:layout_constraintBottom_toBottomOf="@id/view_myPage_distance_frame"
            app:layout_constraintEnd_toStartOf="@id/view_myPage_pace_frame"
            app:layout_constraintStart_toEndOf="@id/view_myPage_distance_frame"
            app:layout_constraintTop_toTopOf="@id/view_myPage_distance_frame" />

        <TextView
            android:id="@+id/tv_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="12.39dp"
            android:text="이동 시간"
            app:layout_constraintEnd_toEndOf="@id/view_myPage_time_frame"
            app:layout_constraintStart_toStartOf="@id/view_myPage_time_frame"
            app:layout_constraintTop_toTopOf="@id/view_myPage_time_frame" />

        <TextView
            android:id="@+id/tv_time_data"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="4.03dp"
            android:text="0:27:36"
            app:layout_constraintEnd_toEndOf="@id/view_myPage_time_frame"
            app:layout_constraintStart_toStartOf="@id/view_myPage_time_frame"
            app:layout_constraintTop_toBottomOf="@id/tv_time" />

        <View
            android:id="@+id/divider_vertical_rightOfTime"
            android:layout_width="0.92dp"
            android:layout_height="37.08dp"
            android:background="@color/G4"
            app:layout_constraintBottom_toBottomOf="@id/view_myPage_time_frame"
            app:layout_constraintStart_toEndOf="@id/view_myPage_time_frame"
            app:layout_constraintTop_toTopOf="@id/view_myPage_time_frame" />

        <!--평균 페이스-->

        <View
            android:id="@+id/view_myPage_pace_frame"
            android:layout_width="112dp"
            android:layout_height="66dp"
            app:layout_constraintBottom_toBottomOf="@id/view_myPage_time_frame"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/view_myPage_time_frame"
            app:layout_constraintTop_toTopOf="@id/view_myPage_time_frame" />

        <TextView
            android:id="@+id/tv_pace"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="12.39dp"
            android:text="평균 페이스"
            app:layout_constraintEnd_toEndOf="@id/view_myPage_pace_frame"
            app:layout_constraintStart_toStartOf="@id/view_myPage_pace_frame"
            app:layout_constraintTop_toTopOf="@id/view_myPage_pace_frame" />


    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>