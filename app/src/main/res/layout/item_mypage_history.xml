<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>
        <!--        <variable-->
        <!--            name=""-->
        <!--            type="" />-->
    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="330dp"
        android:layout_height="wrap_content"
        app:layout_constraintDimensionRatio="330:166"
        android:background="@drawable/radius_9_22dp"
        tools:context=".presentation.mypage.history.MyHistoryFragment">
        <View
            android:id="@+id/view_my_page_history_frame"
            android:layout_width="330dp"
            android:layout_height="0dp"
            app:layout_constraintDimensionRatio="330:166"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
        <ImageView
            android:id="@+id/iv_my_page_history_course"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_marginStart="13.83dp"
            android:layout_marginTop="11.92dp"
            app:layout_constraintStart_toStartOf="@id/view_my_page_history_frame"
            app:layout_constraintTop_toTopOf="@id/view_my_page_history_frame"
            tools:src="@drawable/radius_9_22dp" />

        <TextView
            android:id="@+id/tv_my_page_history_course_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="12.9dp"
            android:layout_marginTop="18.51dp"
            android:text="석촌호수 한 바퀴"
            android:textSize="15.67sp"
            app:layout_constraintStart_toEndOf="@id/iv_my_page_history_course"
            app:layout_constraintTop_toTopOf="@id/iv_my_page_history_course" />

        <TextView
            android:id="@+id/tv_my_page_history_place"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="서울시 강동구"
            android:textSize="11.06sp"
            app:layout_constraintStart_toStartOf="@id/tv_my_page_history_course_name"
            app:layout_constraintTop_toBottomOf="@id/tv_my_page_history_course_name" />


        <View
            android:id="@+id/divider_my_page_history_vertical1"
            android:layout_width="0.92dp"
            android:layout_height="10.61dp"
            android:layout_marginStart="4.99dp"
            android:background="@color/G2"
            app:layout_constraintBottom_toBottomOf="@id/tv_my_page_history_place"
            app:layout_constraintStart_toEndOf="@id/tv_my_page_history_place"
            app:layout_constraintTop_toTopOf="@id/tv_my_page_history_place" />

        <TextView
            android:id="@+id/tv_my_page_history_date"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="4.99dp"
            android:text="2022.12.28"
            android:textSize="11.06sp"
            app:layout_constraintStart_toEndOf="@id/divider_my_page_history_vertical1"
            app:layout_constraintTop_toTopOf="@id/tv_my_page_history_place" />

        <com.google.android.material.divider.MaterialDivider
            android:id="@+id/divider_my_page_history_horizontal1"
            android:layout_width="0dp"
            android:layout_height="0.92dp"
            android:layout_marginHorizontal="10dp"
            android:layout_marginStart="9.22dp"
            android:layout_marginTop="7.38dp"
            android:layout_marginEnd="8.2dp"
            app:layout_constraintEnd_toEndOf="@id/view_my_page_history_frame"
            app:layout_constraintStart_toStartOf="@id/view_my_page_history_frame"
            app:layout_constraintTop_toBottomOf="@id/iv_my_page_history_course" />


        <View
            android:id="@+id/divider_my_page_history_vertical2"
            android:layout_width="0.92dp"
            android:layout_height="36.16dp"
            android:layout_marginStart="111.54dp"
            android:layout_marginTop="11.81dp"
            android:layout_marginEnd="218.46dp"
            android:background="@color/G4"
            android:orientation="vertical"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/divider_my_page_history_horizontal1" />

        <View
            android:id="@+id/divider_my_page_history_vertical3"
            android:layout_width="0.92dp"
            android:layout_height="36.16dp"
            android:layout_marginTop="11.81dp"
            android:layout_marginEnd="113.38dp"
            android:background="@color/G4"
            android:orientation="vertical"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@id/divider_my_page_history_horizontal1" />

        <!--총 거리-->
        <TextView
            android:id="@+id/tv_my_page_history_distance"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="13.39dp"
            android:layout_marginEnd="35.82dp"
            android:text="총 거리"
            android:textColor="@color/G2"
            app:layout_constraintEnd_toEndOf="@id/divider_my_page_history_vertical2"
            app:layout_constraintTop_toBottomOf="@id/divider_my_page_history_horizontal1" />

        <TextView
            android:id="@+id/tv_my_page_history_distance_data"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:layout_marginEnd="3dp"
            tools:text="4.01"
            android:textColor="@color/G1"
            android:textSize="15.67sp"
            android:fontFamily="@font/pretendard_semibold"
            app:layout_constraintEnd_toStartOf="@id/tv_my_page_history_distance_km"
            app:layout_constraintTop_toBottomOf="@id/tv_my_page_history_distance" />
        <TextView
            android:id="@+id/tv_my_page_history_distance_km"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="3dp"
            android:layout_marginEnd="24.54dp"
            android:text="km"
            android:textSize="15.67sp"
            android:textColor="@color/G1"
            app:layout_constraintEnd_toEndOf="@id/divider_my_page_history_vertical2"
            app:layout_constraintTop_toBottomOf="@id/tv_my_page_history_distance" />
        <!--이동시간-->
        <TextView
            android:id="@+id/tv_my_page_history_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="13.39dp"
            android:layout_marginStart="32.46dp"
            android:layout_marginEnd="27.62dp"
            android:textColor="@color/G2"
            android:text="이동 시간"
            app:layout_constraintStart_toEndOf="@id/divider_my_page_history_vertical2"
            app:layout_constraintEnd_toEndOf="@id/divider_my_page_history_vertical3"
            app:layout_constraintTop_toBottomOf="@id/divider_my_page_history_horizontal1" />
        <TextView
            android:id="@+id/tv_my_page_history_time_data"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:layout_marginEnd="24.62dp"
            android:fontFamily="@font/pretendard_semibold"
            android:textSize="15.67sp"
            android:text="0:27:36"
            android:textColor="@color/G1"
            app:layout_constraintEnd_toStartOf="@id/divider_my_page_history_vertical3"
            app:layout_constraintTop_toBottomOf="@id/tv_my_page_history_time" />
        <!--평균 페이스-->
        <TextView
            android:id="@+id/tv_my_page_history_pace"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="13.39dp"
            android:layout_marginStart="27.38dp"
            android:textColor="@color/G2"
            tools:text="평균 페이스"
            app:layout_constraintStart_toEndOf="@id/divider_my_page_history_vertical3"
            app:layout_constraintTop_toTopOf="@id/divider_my_page_history_horizontal1" />

        <TextView
            android:id="@+id/tv_my_page_history_pace_data"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:layout_marginEnd="40dp"
            android:fontFamily="@font/pretendard_semibold"
            android:textSize="15.67sp"
            tools:text="6'45''"
            android:textColor="@color/G1"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@id/tv_my_page_history_time" />


    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>